{
  "name": "woof-client",
  "version": "1.0.0",
  "description": "A command line WOOF client built with Node.js",
  "main": "dist/run.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive",
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "start": "node entrypoint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joelove/woof-client.git"
  },
  "author": "Joe Love",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joelove/woof-client/issues"
  },
  "homepage": "https://github.com/joelove/woof-client#readme",
  "preferGlobal": true,
  "bin": {
    "woof": "dist/run.js"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.7.2",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^2.5.3"
  }
